{
  "name": "william-goode-portfolio",
  "version": "1.0.0",
  "description": "Academic portfolio site with Jekyll and React",
  "scripts": {
    "build": "webpack --mode production",
    "dev": "webpack --mode development --watch",
    "jekyll:build": "bundle exec jekyll build",
    "jekyll:serve": "bundle exec jekyll serve --livereload",
    "serve": "npm run build && npm run jekyll:serve"
  },
  "keywords": ["jekyll", "react", "portfolio"],
  "author": "William Goode",
  "license": "MIT",
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/preset-env": "^7.23.0",
    "@babel/preset-react": "^7.22.0",
    "babel-loader": "^9.1.3",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  }
}

